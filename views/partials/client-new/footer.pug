footer.main-footer
    .container
        .main-footer-top
            .main-footer-info
                a(href="/").logo.logo--white.main-footer-info__logo
                p.main-footer-info__text 
                    | #{labels.address1 || "ТОВ \"КОНСАЛТИНГ СИНДИКАТ\""}
                    br
                    | #{labels.address2 || "ЕГРПОУ: 42182763, Адрес: 01033, г. Киев,"}
                    br
                    | #{labels.address3 || "ул. Паньковская, 14"}
                .main-footer-socials
                    //- a(href="#").main-footer-social
                    //-     img(src="/img/client-new/common/socials/vk.svg").main-footer-social__icon
                    a(target="_blank" href="https://facebook.com/geniusmarketingme").main-footer-social
                        img(src="/img/client-new/common/socials/fb.svg").main-footer-social__icon
                    a(target="_blank" href="https://www.youtube.com/user/Olestimofeev").main-footer-social
                        img(src="/img/client-new/common/socials/yt.svg").main-footer-social__icon
                    a(target="_blank" href="https://t.me/genius_space").main-footer-social
                        img(src="/img/client-new/common/socials/tg.svg").main-footer-social__icon
                    a(target="_blank" href="https://www.instagram.com/geniusmarketingofficial/").main-footer-social
                        img(src="/img/client-new/common/socials/in.svg").main-footer-social__icon
            .main-footer-menu-layout
                .main-footer-menu-block
                    p.footer-subtitle.main-footer-menu-title #{labels.footerMenuTitle1 || "О Genius.Space"}
                    .main-footer-menu
                        a(target="_blank" href="https://genius.space/").main-footer-menu__link #{labels.footerLink1 || "Главная"}
