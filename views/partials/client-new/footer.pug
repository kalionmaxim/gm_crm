                        a(target="_blank" href="https://genius.space/lab/").main-footer-menu__link #{labels.footerLink3 || "Полезные образовательные материалы"}
                        a(target="_blank" href="https://genius.space/nashi-programmy-obucheniya/").main-footer-menu__link #{labels.footerLink4 || "Наши программы обучения"}
                        a(target="_blank" href="https://genius.space/otzyvy/").main-footer-menu__link #{labels.footerLink5 || "Отзывы"}
                .main-footer-menu-block
                    p.footer-subtitle.main-footer-menu-title &nbsp;
                    .main-footer-menu
                        a(target="_blank" href="https://genius.space/o-nas/").main-footer-menu__link #{labels.footerLink6 || "О нас"}
                        a(target="_blank" href="https://genius.space/dlya-biznesa/").main-footer-menu__link #{labels.footerLink7 || "Для бизнеса"}
